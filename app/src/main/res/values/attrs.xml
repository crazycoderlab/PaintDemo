<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MyPaintDemoLinearLayout">
        <attr name="inflateChildViewClassName" format="string"/>
        <attr name="openInfo" format="string"/>
        <attr name="unOpenInfo" format="string"/>
    </declare-styleable>

    <declare-styleable name="StyleDemoView">
        <attr name="styleType" format="integer">
            <enum name="stroke" value="1"/>
            <enum name="fill" value="2"/>
            <enum name="fillStroke" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="LineCapDemoView">
        <attr name="capStyle" format="integer">
            <enum name="BUTT" value="0"/>
            <enum name="ROUND" value="1"/>
            <enum name="SQUARE" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="LineJoinDemoView">
        <attr name="joinStyle" format="integer">
            <enum name="MITER" value="0"/>
            <enum name="ROUND" value="1"/>
            <enum name="BEVEL" value="2"/>
        </attr>
    </declare-styleable>

</resources>